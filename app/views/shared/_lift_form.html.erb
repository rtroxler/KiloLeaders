<%= form_for(@lift) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.text_field :name, placeholder: "Snatch/CnJ" %>
    <%= f.text_field :weight, placeholder: "weight (kg)", size: 10 %>
  </div>
  <%= f.submit "Post new lift", class: "btn btn-large btn-primary" %>
<% end %>
